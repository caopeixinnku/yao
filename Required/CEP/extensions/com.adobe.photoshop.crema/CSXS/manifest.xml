<ExtensionManifest ExtensionBundleId="com.adobe.webpa.PSExport" ExtensionBundleVersion="1.0" Version="5.0">
    <ExtensionList>
        <Extension Id="com.adobe.WEBPA.crema.saveforweb" Version="3.1.2" />
        <Extension Id="com.adobe.WEBPA.crema.saveforwebdocument" Version="3.1.2" />
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="PHXS" Version="17" />
            <Host Name="PHSP" Version="17" />
        </HostList>
        <LocaleList>
            <Locale Code="All" />
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="5.0" />
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        
        <Extension Id="com.adobe.WEBPA.crema.saveforweb">

                <Resources>
                    <MainPath>./PSPanel/dialog.html</MainPath>
                    <CEFCommandLine>
                        <Parameter>--enable-nodejs</Parameter>
                    </CEFCommandLine>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>ModalDialog</Type>
                    <Menu>%EXTEN_MENU_TITLE</Menu>
                    <Geometry>
                        <Size>
                            <Height>700</Height>
                            <Width>1100</Width>
                        </Size>
                        <MaxSize>
                            <Height>10000</Height>
                            <Width>10000</Width>
                        </MaxSize>
                        <MinSize>
                            <Height>450</Height>
                            <Width>900</Width>
                        </MinSize>
                    </Geometry>
                </UI>
            </DispatchInfo>
        </Extension>
         
          <Extension Id="com.adobe.WEBPA.crema.saveforwebdocument">
